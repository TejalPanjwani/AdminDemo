<h1> login </h1>


<%= form_with scope: :users ,url: sessions_create_path,method: :get , local: true  do |f| %>
    <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>
    <%= f.text_field :name, placeholder: "Name" %> <br><br>
    <%= f.text_field :password, placeholder: "Password" %> <br><br>
    <%= f.submit "login" %>
<% end %>




<%= link_to "register", register_users_path %>